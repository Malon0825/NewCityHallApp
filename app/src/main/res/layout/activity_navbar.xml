<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/my_root_view"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="start"
    android:background="@drawable/transparent"
    tools:context=".Navbar"
>

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="313dp"
        android:layout_height="642dp"
        android:background="@drawable/sidebar_bg"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <ImageButton
        android:id="@+id/about_us_btn"
        android:layout_width="47dp"
        android:layout_height="47dp"
        android:background="@drawable/user"
        app:layout_constraintBottom_toBottomOf="@+id/imageView2"
        app:layout_constraintEnd_toEndOf="@+id/imageView2"
        app:layout_constraintHorizontal_bias="0.06"
        app:layout_constraintStart_toStartOf="@+id/imageView2"
        app:layout_constraintTop_toTopOf="@+id/imageView2"
        app:layout_constraintVertical_bias="0.026" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_margin="10dp"
        android:fontFamily="sans-serif"
        android:text="ABOUT US"
        android:textColor="@color/black"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/imageView2"
        app:layout_constraintEnd_toEndOf="@+id/imageView2"
        app:layout_constraintHorizontal_bias="0.418"
        app:layout_constraintStart_toStartOf="@+id/about_us_btn"
        app:layout_constraintTop_toBottomOf="@+id/about_us_btn"
        app:layout_constraintVertical_bias="0.024" />

    <TextView
        android:id="@+id/txt_about_us"
        android:layout_width="257dp"
        android:layout_height="328dp"
        android:layout_gravity="center"
        android:layout_margin="10dp"
        android:fontFamily="sans-serif"
        android:gravity="center"
        android:text="@string/about_us"
        android:textColor="@color/black"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView2"
        app:layout_constraintEnd_toEndOf="@+id/line"
        app:layout_constraintStart_toStartOf="@+id/line"
        app:layout_constraintTop_toBottomOf="@+id/line"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/line"
        android:layout_width="270dp"
        android:layout_height="24dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView2"
        app:layout_constraintEnd_toEndOf="@+id/textView"
        app:layout_constraintStart_toStartOf="@+id/textView"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/line" />

    <ImageButton
        android:id="@+id/img_btn_sti"
        android:layout_width="145dp"
        android:layout_height="150dp"
        android:background="@drawable/sti_logo"
        app:layout_constraintBottom_toBottomOf="@+id/imageView2"
        app:layout_constraintEnd_toEndOf="@+id/txt_about_us"
        app:layout_constraintHorizontal_bias="0.132"
        app:layout_constraintStart_toStartOf="@+id/txt_about_us"
        app:layout_constraintTop_toBottomOf="@+id/txt_about_us"
        app:layout_constraintVertical_bias="0.0" />

    <ImageButton
        android:id="@+id/yellowKor"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:background="@drawable/koronadal_logo"
        app:layout_constraintBottom_toBottomOf="@+id/img_btn_sti"
        app:layout_constraintEnd_toEndOf="@+id/txt_about_us"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/img_btn_sti"
        app:layout_constraintTop_toBottomOf="@+id/txt_about_us"
        app:layout_constraintVertical_bias="0.462" />


